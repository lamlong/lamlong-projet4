<?xml version="1.0" encoding="UTF-8"?>
<Configuration status="WARN">
  <Appenders>
    <File name="File" fileName="./logs/parkingSytem.log">
      <PatternLayout>
      	<pattern>"%d{HH:mm:ss.SSS} [%t] %-5level %logger{36} - %msg%n"</pattern>
      </PatternLayout>
    </File>
  </Appenders>
  <Loggers>
    <Root level="warn">
      <AppenderRef ref="File"/>
    </Root>
  </Loggers>
</Configuration>